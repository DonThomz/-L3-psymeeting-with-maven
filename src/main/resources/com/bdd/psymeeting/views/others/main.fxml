<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.*?>
<StackPane fx:id="stackPane" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
           fx:controller="com.bdd.psymeeting.controller.MainController">
    <children>
        <VBox StackPane.alignment="CENTER">
            <children>
                <fx:include source="../others/top_bar.fxml"/>
                <AnchorPane>
                    <children>
                        <VBox fx:id="mainContent" alignment="CENTER" AnchorPane.bottomAnchor="0.0"
                              AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0"/>
                    </children>
                </AnchorPane>
            </children>
        </VBox>
    </children>
</StackPane>
